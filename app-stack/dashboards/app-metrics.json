        {
          "expr": "histogram_quantile(0.95, sum(rate(app_request_latency_seconds_bucket[5m])) by (le, application))",
          "legendFormat": "{{application}} P95",
          "refId": "C"
        }
      ],
      "yaxes": [
        {
          "format": "s",
